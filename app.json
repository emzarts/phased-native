{
  "expo": {
    "name": "phased-native",
    "slug": "phased-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#333333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#333333"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "recreating an old game that no longer exists. It is a clicker game where you press green tiles to earn points and lose the game if you click a red tile. The game becomes progressively faster. ",
    "githubUrl": "https://github.com/emzarts/phased-native"
  }
}
